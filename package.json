{
  "name": "musically-api",
  "version": "0.0.3",
  "description": "Unofficial API wrapper for musical.ly",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf lib",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "test": "mocha",
    "test-with-coverage": "nyc --reporter=text mocha",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "watch": "npm run clean && tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szdc/musically-api.git"
  },
  "keywords": [
    "musically",
    "musical.ly"
  ],
  "author": "Jack Willis-Craig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szdc/musically-api/issues"
  },
  "homepage": "https://github.com/szdc/musically-api#readme",
  "dependencies": {
    "@types/tough-cookie": "^2.3.3",
    "axios": "^0.18.0",
    "axios-cookiejar-support": "^0.3.5",
    "json-bigint": "^0.3.0",
    "tough-cookie": "^2.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.0",
    "axios-mock-adapter": "github:szdc/axios-mock-adapter",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage/**",
      "lib/**",
      "test/**",
      "src/types/**",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
